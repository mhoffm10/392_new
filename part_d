import pickle

#part i.
#creating a class to hold the three attributes
class Order_ship_profit:
    def __init__(self, order_id, ship_date, total_profit):
        self.__order_id = order_id
        self.__ship_date = ship_date
        self.__total_profit = total_profit

    #part ii.
    #accessor methods only, and __str__ method
    def get_order_id(self):
        return self.__order_id
    def get_ship_date(self):
        return self.__ship_date
    def get_total_profit(self):
        return self.__total_profit

    def __str__(self):
        return "Order ID: "+self.__order_id+"\t"+"Ship Date: "+self.__ship_date+"\t"+"Total Profit: "+ self.__total_profit


